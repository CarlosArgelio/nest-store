NODE_ENV                      = ''
# Can be used with "development" | "production" | "staging"

API_KEY                       = 0

# Configuration DB when used a client postgress
PGADMIN_EMAIL                 = ''
PGADMIN_PASSWORD              = ''
PGADMIN_PORT_INSTANCE         = 5050 # recommend
PGADMIN_PORT_IMAGE            = 80 # recommend

# Configuration DB when used postgres
POSTGRES_DB                   = ''
POSTGRES_USER                 = ''
POSTGRES_PASSWORD             = ''
POSTGRES_PORT                 = 5432
POSTGRES_HOST                 = ''

# Configuration DB when used mysql
MYSQL_DB                      = ''
MYSQL_USER                    = ''
MYSQL_PASSWORD                = ''
MYSQL_PORT                    = 3306 # recommend
MYSQL_HOST                    = ''
MYSQL_VERSION                 = '5' # Default
MARIADB_VERSION               = '10.11' # Default

PHPMYADMIN_PORT_INSTANCE      = 8080 # recommend
PHPMYADMIN_PORT_IMAGE         = 80 # recommend

# Config migrations

TYPEORM_CONNECTION            = ''
TYPEORM_HOST                  = ''
TYPEORM_USERNAME              = ''
TYPEORM_PASSWORD              = ''
TYPEORM_DATABASE              = ''
TYPEORM_PORT                  = 5432 # recommend this is equal to POSTGRES_PORT or MYSQL_PORT
TYPEORM_SYNCHRONIZE           = false
TYPEORM_LOGGING               = true
TYPEORM_ENTITIES              = 'src/**/*.entity.ts' # NOT CHANGES

TYPEORM_MIGRATIONS            = 'src/database/migrations/*.ts' # NOT CHANGES
TYPEORM_MIGRATIONS_DIR        = 'src/database/migrations' # NOT CHANGES
TYPEORM_MIGRATIONS_TABLE_NAME = 'migrations' # NOT CHANGES
